{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "example: debug main process",
            "runtimeExecutable": "npm",
            "runtimeArgs": [
                "run",
                "test"
            ],
            "skipFiles": [
                "<node_internals>/**"
            ],
            "cwd": "${workspaceFolder}/example",
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
            "presentation": {
                "hidden": true
            },
        },
        {
            "type": "node",
            "request": "attach",
            "name": "try debug child process",
            "port": 12016,
            "skipFiles": [
                "<node_internals>/**"
            ],
            "timeout": 5000,
            "restart": false,
            "presentation": {
                "hidden": true
            },
        },
    ],
    "compounds": [
        {
            "name": "(example) debug tests",
            "configurations": ["example: debug main process", "try debug child process"],
            "presentation": {
                "hidden": false,
                "group": "",
                "order": 1
            },
        }
    ]
}
